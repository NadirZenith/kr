{
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]
      }],
      "max-len": ["error", { "ignoreTrailingComments": true, "ignoreTemplateLiterals": true }]
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    }
}
